/* components/custom-tabbar/index.wxss */
.box_10 {
  position: absolute;
  left: 30rpx;
  bottom: 74rpx;
  width: 690rpx;
  height: 156rpx;
  background: url(data:image/png;base64,) 0rpx 0rpx no-repeat;
  background-size: 690rpx 158rpx;
  flex-direction: row;
  display: flex;
  justify-content: space-between;
  padding: 16rpx 80rpx 16rpx 82rpx;
}
.image-text_5 {
  margin-top: 46rpx;
  display: flex;
  flex-direction: column;
}
.thumbnail_10 {
  width: 34rpx;
  height: 40rpx;
  margin: 0 20rpx 0 18rpx;
}
.text-group_6 {
  overflow-wrap: break-word;
  color: #EDE7D9;
  font-size: 24rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: 500;
  text-align: left;
  white-space: nowrap;
  line-height: 24rpx;
  margin-top: 14rpx;
}
.image-text_6 {
  display: flex;
  flex-direction: column;
}
.image-wrapper_1 {
  background-color: rgba(245,186,220,1.000000);
  border-radius: 100%;
  display: flex;
  flex-direction: column;
  margin: 0 18rpx 0 16rpx;
  padding: 24rpx 22rpx 22rpx 20rpx;
  display: flex;
  justify-content: center; /* 水平居中 */
  align-items: center; /* 垂直居中 */
}
.image_3 {
  width: 44rpx;
  height: 40rpx;
}
.text-group_7 {
  overflow-wrap: break-word;
  color: #EDE7D9;
  font-size: 24rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: normal;
  text-align: left;
  white-space: nowrap;
  line-height: 24rpx;
  margin-top: 14rpx;
}
.image-text_7 {
  margin-top: 48rpx;
  display: flex;
  flex-direction: column;
}
.image_4 {
  width: 38rpx;
  height: 42rpx;
  margin: 0 16rpx 0 18rpx;
}
.text-group_8 {
  overflow-wrap: break-word;
  color: #EDE7D9;
  font-size: 24rpx;
  font-family: PingFangSC, PingFang SC;
  font-weight: normal;
  text-align: left;
  white-space: nowrap;
  line-height: 24rpx;
  margin-top: 10rpx;
}
/* 新增激活状态样式 */
.image-text_5.active .thumbnail_10,
.image-text_7.active .image_4 {
  filter: brightness(1.2); /* 图标高亮效果 */
}

.image-text_6.active .image_3 {
  width: 48rpx;
  height: 44rpx; /* 激活时图标放大 */
}

/* 文字激活状态 */
.image-text_5.active .text-group_6,
.image-text_6.active .text-group_7,
.image-text_7.active .text-group_8 {
  font-family: PingFangSC, PingFang SC;
  font-weight: 500;
  font-size: 24rpx;
  color: #F5BADC;
  line-height: 24rpx;
  font-style: normal;
}

/* 中间按钮激活状态 */
.image-wrapper_1.active-middle {
  background-color: rgba(245, 186, 220, 0.8) !important;
  transform: scale(1.1);
  transition: all 0.3s ease;
}